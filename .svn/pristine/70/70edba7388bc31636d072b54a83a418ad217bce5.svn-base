/* left menu */


.glossymenu, .glossymenu li ul
{
	list-style-type: none;
	margin: 0;
	padding: 0;
	width: 271px;
	margin-top: 5px;
	background-color: #2D64AC;  /*#32784c;*/ /*WIDTH OF MAIN MENU ITEMS*/
}

.glossymenu li
{
	position: relative;
	margin-bottom: 4px;
}

.glossymenu li a
{
	background-image: url(images/verticalmenubg1.png);
	background-repeat: no-repeat;
	font: bold 12px Verdana, Helvetica, sans-serif;
	color: white;
	display: block;
	width: 236px;
	padding: 5px 0;
	padding-left: 35px;
	text-decoration: none;
}

.glossymenu li ul
{
	/*SUB MENU STYLE*/
	position: absolute;
	width: 271px;
	height: auto; /*WIDTH OF SUB MENU ITEMS*/
	left: 0;
	top: 0;
	display: none;
}

.glossymenu li ul li
{
	float: left;
}

.glossymenu li ul a
{
	width: 271px; /*WIDTH OF SUB MENU ITEMS - 10px padding-left for A elements */
}

.glossymenu .arrowdiv
{
	position: absolute;
	right: 2px;
	background: transparent url(images/arrow.gif) no-repeat center right;
}

.glossymenu li a:visited, .glossymenu li a:active
{
	color: white;
}

.glossymenu li a:hover
{
	background-image: url(Images/verticalmenubg2.png);
}

/* Holly Hack for IE \*/
* html .glossymenu li
{
	float: left;
	height: 1%;
}
* html .glossymenu li a
{
	height: 1%;
}
/* End */
